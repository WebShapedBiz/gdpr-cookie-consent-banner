{
  "name": "gdpr-cookie-consent-banner",
  "version": "4.0.0",
  "description": "A full featured cookie consent plugin which meets GDPR, DSGVO and other requirements.",
  "keywords": [
    "gdpr",
    "dsgvo",
    "cookie",
    "consent",
    "banner"
  ],
  "homepage": "https://muuvmuuv.github.io/gdpr-cookie-consent-banner",
  "bugs": "https://github.com/muuvmuuv/gdpr-cookie-consent-banner/issues",
  "repository": "git@github.com:muuvmuuv/gdpr-cookie-consent-banner.git",
  "license": "MIT",
  "author": {
    "name": "Marvin Heilemann",
    "email": "marvin.heilemann+github@googlemail.com",
    "url": "https://marvin.digital/"
  },
  "source": "lib/index.js",
  "main": "dist/cookie-consent.js",
  "umd:main": "dist/cookie-consent.umd.js",
  "module": "dist/cookie-consent.mjs",
  "unpkg": "dist/cookie-consent.umd.js",
  "scripts": {
    "bundle": "microbundle --name CookieConsent",
    "prepublishOnly": "pnpm run bundle",
    "build": "preact build --src docs --preload --no-esm",
